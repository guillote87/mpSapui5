{
  "welcomeFile": "/destination_service_frontend/index.html",
  "authenticationMethod": "none",
  "logout": {
    "logoutEndpoint": "/do/logout"
  },
  "routes": [
    {
      "source": "^/destination_service_frontend/(.*)$",
      "target": "$1",
      "localDir": "webapp"
    },
    {
      "source": "^/getUsers",
      "target": "/users",
      "destination": "getUsers"
    }
  ]
}